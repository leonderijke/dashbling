{
  "name": "@dashbling/build-support",
  "version": "0.2.1",
  "author": "Pascal Widdershoven",
  "description": "Hackable React based dashboards for developers",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pascalw/dashbling.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc -p .",
    "prepare": "yarn run --silent build && cp ../../README.md .",
    "test": "yarn run --silent build && jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@dashbling/core": "^0.2.1",
    "autoprefixer": "^7.1.6",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^4.29.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.57",
    "jest": "^24",
    "ts-jest": "^23.10.5",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@dashbling)/)"
    ],
    "testRegex": "((\\.|/)test)\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "engines": {
    "node": ">= 10"
  }
}
